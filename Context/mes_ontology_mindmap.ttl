mes:UnplannedDowntime a owl:Class ;
    rdfs:subClassOf mes:DowntimeReason ;
    rdfs:label "UnplannedDowntime" ;
    rdfs:comment "Unexpected production stoppages" ;
    mes:businessContext "Unexpected production stoppages. Major focus area for continuous improvement." .


