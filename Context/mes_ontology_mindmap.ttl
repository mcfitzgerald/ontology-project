mes:Changeover a owl:Class ;
    rdfs:subClassOf mes:PlannedDowntime ;
    rdfs:label "Changeover" ;
    rdfs:comment "Product switch requiring reconfiguration" ;
    mes:businessContext "Product switch requiring equipment reconfiguration. Typical duration: 30-60 minutes. Code: PLN-CO." .

mes:Cleaning a owl:Class ;
    rdfs:subClassOf mes:PlannedDowntime ;
    rdfs:label "Cleaning" ;
    rdfs:comment "Mandatory sanitation cycle" ;
    mes:businessContext "Mandatory sanitation cycle every 8 hours for food safety. Duration: 30 minutes. Code: PLN-CLN." .

mes:DowntimeLog a owl:Class ;
    rdfs:subClassOf mes:Event ;
    rdfs:label "DowntimeLog" ;
    rdfs:comment "Equipment stoppage event" ;
    mes:businessContext "Equipment stoppage event with reason code. Critical for availability calculation and improvement initiatives." .

mes:DowntimeReason a owl:Class ;
    rdfs:subClassOf mes:Reason ;
    rdfs:label "DowntimeReason" ;
    rdfs:comment "Coded explanation for equipment stoppage" ;
    mes:businessContext "Coded explanation for equipment stoppage. Primary driver of availability losses in OEE." .

mes:ElectricalFailure a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "ElectricalFailure" ;
    rdfs:comment "Power or control system malfunction" ;
    mes:businessContext "Power or control system malfunction. Can cascade to entire line. Code: UNP-ELEC." .

mes:Equipment a owl:Class ;
    rdfs:subClassOf mes:Resource ;
    rdfs:label "Equipment" ;
    rdfs:comment "Physical machines that transform raw materials" ;
    mes:businessContext "Physical machines that transform raw materials into finished products. Performance tracked in 5-minute intervals." .

mes:Event a owl:Class ;
    rdfs:label "Event" ;
    rdfs:comment "Time-stamped production occurrence" ;
    mes:businessContext "Time-stamped production occurrence captured every 5 minutes. Foundation for all KPI calculations." .

mes:Filler a owl:Class ;
    rdfs:subClassOf mes:Equipment ;
    rdfs:label "Filler" ;
    rdfs:comment "First stage in bottling line" ;
    mes:businessContext "First stage in bottling line. Fills containers with product. Critical for volume accuracy and product quality. Common issues: incorrect fill levels, foaming, contamination." .

mes:MaterialJam a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "MaterialJam" ;
    rdfs:comment "Product blockage in equipment" ;
    mes:businessContext "Product blockage in equipment. Most frequent unplanned stop (35% probability on LINE2-PCK). Duration: 0.5-2 minutes. Code: UNP-JAM." .

mes:MaterialStarvation a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "MaterialStarvation" ;
    rdfs:comment "Upstream supply shortage" ;
    mes:businessContext "Upstream supply shortage. Peak times: 10-12am and 3-5pm. Also caused by cascade failures from upstream equipment after 10-minute delay. Code: UNP-MAT." .

mes:MechanicalFailure a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "MechanicalFailure" ;
    rdfs:comment "Equipment breakdown requiring repair" ;
    mes:businessContext "Equipment breakdown requiring repair. Major impact on availability. Can exceed 5 hours. Code: UNP-MECH." .

mes:OperatorError a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "OperatorError" ;
    rdfs:comment "Human-caused stoppage" ;
    mes:businessContext "Human-caused stoppage. 12% probability on night shift (10pm-6am). Code: UNP-OPR." .

mes:Packer a owl:Class ;
    rdfs:subClassOf mes:Equipment ;
    rdfs:label "Packer" ;
    rdfs:comment "Groups individual containers" ;
    mes:businessContext "Groups individual containers into cases/multipacks. Common issues: material jams, label misalignment, case damage." .

mes:Palletizer a owl:Class ;
    rdfs:subClassOf mes:Equipment ;
    rdfs:label "Palletizer" ;
    rdfs:comment "Stacks cases onto pallets" ;
    mes:businessContext "Stacks cases onto pallets for shipping. End of production line. Common issues: unstable stacks, stretch wrap failures." .

mes:PlannedDowntime a owl:Class ;
    rdfs:subClassOf mes:DowntimeReason ;
    rdfs:label "PlannedDowntime" ;
    rdfs:comment "Scheduled production interruptions" ;
    mes:businessContext "Scheduled production interruptions. Predictable but still impacts availability score." .

mes:PreventiveMaintenance a owl:Class ;
    rdfs:subClassOf mes:PlannedDowntime ;
    rdfs:label "PreventiveMaintenance" ;
    rdfs:comment "Scheduled equipment service" ;
    mes:businessContext "Scheduled equipment service to prevent breakdowns. Weekly/monthly cycles. Code: PLN-PM." .

mes:Process a owl:Class ;
    rdfs:label "Process" ;
    rdfs:comment "Manufacturing workflow that transforms raw materials into finished products through defined operations" ;
    mes:businessContext "Manufacturing workflow that transforms raw materials into finished products through defined operations." .

mes:Product a owl:Class ;
    rdfs:subClassOf mes:Resource ;
    rdfs:label "Product" ;
    rdfs:comment "Items manufactured for sale" ;
    mes:businessContext "Items manufactured for sale. Each has target rates, costs, and quality specifications." .

mes:ProductionLine a owl:Class ;
    rdfs:subClassOf mes:Resource ;
    rdfs:label "ProductionLine" ;
    rdfs:comment "Complete set of equipment working together" ;
    mes:businessContext "Complete set of equipment working together. Throughput limited by slowest equipment (bottleneck)." .

mes:ProductionLog a owl:Class ;
    rdfs:subClassOf mes:Event ;
    rdfs:label "ProductionLog" ;
    rdfs:comment "5-minute snapshot when equipment is running" ;
    mes:businessContext "5-minute snapshot of production metrics when equipment is running. Core data for OEE calculation." .

mes:ProductionOrder a owl:Class ;
    rdfs:subClassOf mes:Process ;
    rdfs:label "ProductionOrder" ;
    rdfs:comment "Customer-driven manufacturing request" ;
    mes:businessContext "Customer-driven manufacturing request specifying product type, quantity, and schedule. Typical duration: 4-12 hours." .

mes:QualityCheck a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "QualityCheck" ;
    rdfs:comment "Production halt for quality inspection" ;
    mes:businessContext "Production halt for quality inspection. Protects brand reputation. Code: UNP-QC." .

mes:Reason a owl:Class ;
    rdfs:label "Reason" ;
    rdfs:comment "Categorized explanation for production events" ;
    mes:businessContext "Categorized explanation for production events, especially equipment stoppages. Critical for root cause analysis." .

mes:Resource a owl:Class ;
    rdfs:label "Resource" ;
    rdfs:comment "Assets required for production" ;
    mes:businessContext "Assets required for production including equipment, production lines, and product specifications." .

mes:SensorFailure a owl:Class ;
    rdfs:subClassOf mes:UnplannedDowntime ;
    rdfs:label "SensorFailure" ;
    rdfs:comment "Misaligned or faulty sensors" ;
