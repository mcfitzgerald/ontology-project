# Google API Configuration
GOOGLE_API_KEY=your-google-api-key-here
DEFAULT_MODEL=gemini-2.0-flash
MODEL_TEMPERATURE=0.1

# SPARQL API Configuration
SPARQL_ENDPOINT=http://localhost:8000/sparql/query
SPARQL_TIMEOUT=30
SPARQL_MAX_RESULTS=10000

# Cache Configuration
CACHE_ENABLED=true
CACHE_TTL=3600
CACHE_DIR=./adk_agents/cache

# Analysis Configuration
ONTOLOGY_NAMESPACE=mes_ontology_populated

# Logging Configuration
LOG_LEVEL=INFO
ADK_DEBUG=false

# Session Management
SESSION_TIMEOUT_HOURS=24
SESSION_SIZE_WARNING=100

# API Configuration (for SPARQL service)
API_HOST=0.0.0.0
API_PORT=8000
CORS_ENABLED=true
CORS_ORIGINS=["*"]

# Performance Settings
MAX_CONTEXT_TOKENS=32000
RESULT_SUMMARY_THRESHOLD=10000
CACHE_CLEANUP_DAYS=7